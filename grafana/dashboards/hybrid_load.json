{
  "title": "Hybrid Load Overview",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "k6 HTTP req duration (avg)",
      "targets": [
        {
          "measurement": "http_req_duration",
          "groupBy": [
            {
              "type": "time",
              "params": [
                "1s"
              ]
            }
          ],
          "select": [
            [
              {
                "type": "field",
                "params": [
                  "value"
                ]
              },
              {
                "type": "mean",
                "params": []
              }
            ]
          ]
        }
      ],
      "datasource": "InfluxDB",
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "graph",
      "title": "Gatling Requests/sec (graphite->influx)",
      "targets": [
        {
          "measurement": "gatling.requests.count",
          "groupBy": [
            {
              "type": "time",
              "params": [
                "1s"
              ]
            }
          ],
          "select": [
            [
              {
                "type": "field",
                "params": [
                  "value"
                ]
              },
              {
                "type": "mean",
                "params": []
              }
            ]
          ]
        }
      ],
      "datasource": "InfluxDB",
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ]
}